<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="sdm_playerseason_tree" model="ir.ui.view">
	<field name="name">sdm.playerseason.view.tree</field>
	<field name="model">sdm.playerseason</field>
	<field name="arch" type="xml">
		<tree>
			<field name="players_id"/>
			<field name="season_id"/>
			<field name="team_id"/>
		</tree>
	</field>
</record>

<record id="sdm_playerseason_form" model="ir.ui.view">
	<field name="name">sdm.playerseason.view.form</field>
	<field name="model">sdm.playerseason</field>
	<field name="arch" type="xml">
		<form>
			<sheet>
				<group>
					<field name="players_id"/>
					<field name="season_id"/>
					<field name="team_id"/>
				</group>
			</sheet>
		</form>
	</field>
</record>

<record id="sdm_playerseason_action" model="ir.actions.act_window">
	<field name="name">sezóny hráčov</field>
	<field name="type">ir.actions.act_window</field>
	<field name="res_model">sdm.playerseason</field>
	<field name="view_mode">tree,form</field>
</record>
</odoo>